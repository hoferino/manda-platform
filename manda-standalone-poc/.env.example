# Manda-Standalone Environment Variables
# Copy this file to .env and fill in your API keys

# LLM API Keys
ANTHROPIC_API_KEY=your_anthropic_key_here
GOOGLE_API_KEY=your_google_key_here

# LLM Configuration
LLM_PROVIDER=anthropic  # Options: anthropic, google
LLM_MODEL=claude-sonnet-4-5-20250929  # Or gemini-3.0-pro

# Application Settings
LOG_LEVEL=INFO
DEBUG=false
